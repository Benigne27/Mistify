{
  "expo": {
    "name": "Mistify",
    "slug": "Mistify",
    "version": "1.0.0",
    "orientation": "portrait",
    "icon": "./assets/images/MistIcon2.png",
    "scheme": "myapp",
    "userInterfaceStyle": "automatic",
    "newArchEnabled": true,
    "ios": {
      "supportsTablet": true
    },
    "android": {
      "adaptiveIcon": {
        "foregroundImage": "./assets/images/MistIcon2.png",
        "backgroundColor": "#5D3FD3"
      }
    },
    "web": {
      "bundler": "metro",
      "output": "static",
      "favicon": "./assets/images/MistIcon2.png"
    },
    "plugins": [
      "expo-router",
      [
        "expo-splash-screen",
        {
          "image": "./assets/images/Mistify.png",
          "imageWidth": 400,
          "resizeMode": "contain",
          "backgroundColor": "#5D3FD3" 
        }
      ]
    ],
    "experiments": {
      "typedRoutes": true
    },
    "extra": {
      "router": {
        "origin": false
      },
      "eas": {
        "projectId": "1525df8c-ef47-4e0a-90d3-3cd7af7c18fa"
      }
    },
    "runtimeVersion": {
      "policy": "appVersion"
    },
    "updates": {
      "url": "https://u.expo.dev/1525df8c-ef47-4e0a-90d3-3cd7af7c18fa"
    }
  }
}
